CLANG_MODULES_AUTOLINK = YES
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/CTCamera" "${PODS_ROOT}/Headers/Public/GPUImage"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/CTCamera" -isystem "${PODS_ROOT}/Headers/Public/GPUImage"
OTHER_LDFLAGS = $(inherited) -ObjC -l"CTCamera" -l"GPUImage" -framework "AVFoundation" -framework "CoreMedia" -framework "MapKit" -framework "OpenGLES" -framework "QuartzCore" -framework "UIKit"
PODS_ROOT = ${SRCROOT}/Pods